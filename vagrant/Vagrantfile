Vagrant.configure("2") do |config|
  config.vm.box = "ubuntu/xenial64"
  config.vm.network "private_network", ip: "192.168.33.10"
  config.vm.provider "virtualbox" do |vb|
    vb.memory = "8056"
    vb.cpus = 4
  config.vm.provider :virtualbox do |vb|
        vb.name = "spinnaker"
  end
end
  config.vm.provision "shell", path: "install-spinnaker.sh"
end